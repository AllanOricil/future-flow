{
    "name": "future-flow",
    "version": "0.0.1-36",
    "description": "Flow diagram creator with HTML5 Canvas",
    "main": "/dist/future-flow.js",
    "module": "/dist/future-flow.js",
    "browser": "/dist/future-flow.js",
    "homepage": "https://github.com/AllanOricil/future-flow#readme",
    "scripts": {
        "minify": "babel index.js -d dist",
        "uglify": "uglifyjs dist/index.js -o dist/index.min.js -c -m",
        "build": "babel index.js -d dist && uglifyjs dist/index.js -o dist/index.min.js -c -m",
        "build2": "rollup src/index.js -f es -d dist2 && babel index.js -d dist2 && uglifyjs dist2/index.js -o dist2/index.min.js -c -m",
        "build3": "rollup src/index.js -f es -d dist2 && node gulp.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AllanOricil/future-flow"
    },
    "unpkg": "./dist/index.min.js",
