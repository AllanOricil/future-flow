    "keywords": [
        "flow",
        "flow generator",
        "flow builder",
        "flochart",
        "flow diagram",
        "diagram",
        "javascript",
        "canvas",
        "Nuxtjs",
        "Vuejs"
    ],
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "babel-preset-env": "^1.7.0",
        "prettier-eslint": "^9.0.1",
        "terser-webpack-plugin": "^3.0.1",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    }
}